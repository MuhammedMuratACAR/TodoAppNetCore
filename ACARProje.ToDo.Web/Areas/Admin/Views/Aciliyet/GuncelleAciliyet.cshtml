@model AciliyetUpdateViewModel
@{
    ViewData["Title"] = "GuncelleAciliyet";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}


<form asp-action="GuncelleAciliyet" class="w-75 mx-auto p-3" method="post">

    <div class="form-group">
        <input type="hidden" asp-for="Id" />
        <label asp-for="Tanim"></label>
        <span asp-validation-for="Tanim" class="text-danger"></span>
        <input type="text" asp-for="Tanim" class="form-control" />
    </div>
    <button class="btn btn-info btn-sm" type="submit">Kaydet</button>
</form>
@section JavaScript{
    <script src="~/lib/jqueryValidate/jquery.validate.min.js"></script>
    <script src="~/lib/jqueryUnob/jquery.validate.unobtrusive.min.js"></script>
}

